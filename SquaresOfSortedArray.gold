   for i = 1 to arrayLen
      for curElement = 0 to arrayLen - 1
         if inputArray[curElement] > inputArray[curElement + 1]
            temp = inputArray[curElement]
            inputArray[curElement] = inputArray[curElement + 1]
            inputArray[curElement + 1] = temp
         else
            inputArray[curElement] = inputArray[curElement]
         endIf
      endFor
   endFor
endProc 

