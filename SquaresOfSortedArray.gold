class Leetcode (aFullObject) 

type tArrayOf10Int4 : array [0 to 10] of Int4


procedure SquareArrayElements(arrayLen : Int4, inOut inputArray : tArrayOf10Int4)
   var temp : Int4
   var curElement : Int4
   
   for curElement = 0 to arrayLen
      temp = inputArray[curElement] * inputArray[curElement]
      inputArray[curElement] = temp
      WriteLn(inputArray[curElement])
   endFor
endProc 

procedure Abs(inOut inputInt4 : Int4)
   if inputInt4 < 0
      inputInt4 = inputInt4 * -1
   endIf
endProc 

